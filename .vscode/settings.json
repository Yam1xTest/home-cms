{
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
      "javascript",
      "typescript", 
    ],
    "[typescript]": {
      "editor.formatOnSave": true
    },
    "files.eol": "\n",
    "editor.tabSize": 2,
    "editor.codeActionsOnSave": { 
      "source.fixAll.eslint": "explicit",
    },
} 
